<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace></RootNamespace>
    <NoWarn>CS0649;CS0169</NoWarn>
    <NukeRootDirectory>..</NukeRootDirectory>
    <NukeScriptDirectory>..</NukeScriptDirectory>
    <NukeTelemetryVersion>1</NukeTelemetryVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Nuke.Common" Version="6.0.0" />
    <PackageDownload Include="GitVersion.Tool" Version="[5.6.3]" />
    <PackageDownload Include="Nuget.CommandLine" Version="[5.8.1]" />
    <PackageDownload Include="wix" Version="[3.11.2]" />
  </ItemGroup>

  <ItemGroup>
    <None Include="../**/Directory.*.*" LinkBase="Directory.*.*" />
  </ItemGroup>
</Project>
